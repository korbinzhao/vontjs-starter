{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "npm run build",
    "outputDirectory": "",
    "installCommand": "npm install",
    "framework": "Other",
    "builds": [{
        "src": "dist/server/index.js",
        "use": "@vercel/node"
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/server/index.js"
        }
    ]
}