{
    "version": 2,
    "outputDirectory": "dist",
    "installCommand": "npm install && npm run build",
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server/index.js"
        }
    ]
}